{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "WriteTo": [
    {
      "Name": "File",
      "Args": {
        "path": ".logs/log-.txt",
        "rollingInterval": "Day"
      }
    }
  ],
  "AllowedHosts": "*"
}
